# Binaries
/system/bin/chargeonlymode                      u:object_r:charger_exec:s0

# Camera
/data/vendor/qcam(/.*)?                         u:object_r:camera_socket:s0
/(product|system/product)/(lib|lib64)/vendor\.qti\.hardware\.camera\.device@1\.0\.so   u:object_r:same_process_hal_file:s0

# Display
/(product|system/product)/(lib|lib64)/vendor\.display\.config@1\.[0-9]\.so   u:object_r:same_process_hal_file:s0

# Fpc Fingerprint
/data/fpc(/.*)?					u:object_r:fpc_data_file:s0
/dev/socket/fpce(/.*)?				u:object_r:fpce_socket:s0
/sys/devices/soc/soc:fpc1020(/.*)?		u:object_r:fpc_sysfs:s0

# Glove Mode
/sys/class/tp_glove/device(/.*)?		u:object_r:proc_touchpanel:s0
/sys/devices/virtual/tp_glove(/.*)?		u:object_r:proc_touchpanel:s0

# Device Parts
/proc/touchpanel(/.*)?                          u:object_r:proc_touchpanel:s0
